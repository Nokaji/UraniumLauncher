<div class="frame-bar">
    <div class="frame-resizable"></div>
    <div class="frame-content">
        <% if(process.platform === 'darwin') { %>
            <div class="content-darwin-button">
                <button class="frame-button frame-darwin-button" id="frame-button-close" tabIndex="-1"></button>
                <button class="frame-button frame-darwin-button" id="frame-button-minimize" tabIndex="-1"></button>
                <button class="frame-button frame-darwin-button" id="frame-button-restoredown" tabIndex="-1"></button>
            </div>
        <% } else { %> 
            <p class="title" id="title-frame"></p>
            <div class="content-win-button">
                <button class="frame-button" id="frame-button-minimize" tabIndex="-1">
                    <svg width="10" height="10" viewBox="0 0 12 12"><rect stroke="#ffffff" fill="#ffffff" width="10" height="1" x="1" y="6"></rect></svg>
                </button>
                <button class="frame-button" id="frame-button-restoredown" tabIndex="-1">
                    <svg width="10" height="10" viewBox="0 0 12 12"><rect width="9" height="9" x="1.5" y="1.5" fill="none" stroke="#ffffff" stroke-width="1.4px"></rect></svg>
                </button>
                <button class="frame-button" id="frame-button-close" tabIndex="-1">
                    <svg width="10" height="10" viewBox="0 0 12 12"><polygon stroke="#ffffff" fill="#ffffff" fill-rule="evenodd" points="11 1.576 6.583 6 11 10.424 10.424 11 6 6.583 1.576 11 1 10.424 5.417 6 1 1.576 1.576 1 6 5.417 10.424 1"></polygon></svg>
                </button>
            </div>
        <% } %>
    </div>
</div>
<script src="./assets/js/frame.js"></script>